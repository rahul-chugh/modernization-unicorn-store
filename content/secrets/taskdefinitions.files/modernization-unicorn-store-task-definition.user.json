{
  "family": "modernization-unicorn",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "modernization-unicorn-store_unicornstore",
      "image": "444039259723.dkr.ecr.us-east-1.amazonaws.com/modernization-unicorn-store:latest",
      "cpu": 512,
      "memoryReservation": 1024,
      "portMappings": [{
        "containerPort": 80
      }],
      "environment": [
        {
          "name": "ASPNETCORE_ENVIRONMENT",
          "value": "Production"
        }
      ],
      "secrets": [
        {
          "name": "UNICORNSTORE_DBSECRET",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:444039259723:secret:UNICORNSTORE_DBSECRET-MUJPIa"
        },
        {
          "name": "DefaultAdminUsername",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:444039259723:secret:DefaultAdminUsername-XbWTvt"
          
        },
        {
          "name": "DefaultAdminPassword",
          "valueFrom" : "arn:aws:secretsmanager:us-east-1:444039259723:secret:DefaultAdminPassword-W1RmLT"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "UnicornStore",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "web"
        }
      }
    }
  ],
  "executionRoleArn": "arn:aws:iam::444039259723:role/UnicornStoreExecutionRole",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "1 vcpu",
  "memory": "2 gb"
}
