{
  "family": "modernization-unicorn",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "modernization-unicorn-store_unicornstore",
      "image": "IMAGEID",
      "cpu": 512,
      "memoryReservation": 1024,
      "portMappings": [
        {
          "containerPort": 80
        }
      ],
      "environment": [
        {
          "name": "ASPNETCORE_ENVIRONMENT",
          "value": "Production"
        }
      ],
      "secrets": [
        {
          "name": "UnicornDbConnectionStringBuilder__Server",
          "valueFrom": "arn:aws:secretsmanager:us-west-2:131729831033:secret:UnicornDbServer-EJwWpJ"
        },
        {
          "name": "UnicornDbConnectionStringBuilder__Username",
          "valueFrom": "arn:aws:secretsmanager:us-west-2:131729831033:secret:UnicornDbUsername-caTIrh"
        },
        {
          "name": "UnicornDbConnectionStringBuilder__Password",
          "valueFrom": "arn:aws:secretsmanager:us-west-2:131729831033:secret:UnicornDbPassword-paVz6e"
        },
        {
          "name": "DefaultAdminUsername",
          "valueFrom": "arn:aws:secretsmanager:us-west-2:131729831033:secret:DefaultAdminUsername-dCWk7"

        },
        {
          "name": "DefaultAdminPassword",
          "valueFrom": "arn:aws:secretsmanager:us-west-2:131729831033:secret:UnicornDbPassword-paVz6e"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "UnicornStore",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "web"
        }
      }
    }
  ],
  "executionRoleArn": "arn:aws:iam::131729831033:role/UnicornStoreExecutionRole",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "1 vcpu",
  "memory": "2 gb"
}
